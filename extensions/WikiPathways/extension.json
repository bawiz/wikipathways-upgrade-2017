		"WikiPathways\\ThumbPathwaysPager": "src/ThumbPathwaysPager.php",
		"WikiPathways\\TissueAnalyzer": "src/TissueAnalyzer.php",
		"WikiPathways\\Wish": "src/Wish.php",
		"WikiPathways\\Xref": "src/Xref.php",
